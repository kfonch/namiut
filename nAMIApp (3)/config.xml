        <!-- iPhone / iPod Touch  -->
        <icon src="res/ios/AppIcon.appiconset/icon-60.png" width="60" height="60" />
        <icon src="res/ios/AppIcon.appiconset/Icon-App-60x60@2x.png" width="120" height="120" />
